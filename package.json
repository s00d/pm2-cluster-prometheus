{
  "name": "pm2-cluster-prometheus-next",
  "version": "1.3.1",
  "description": "Aggregate node.js workers' metrics when use pm2 cluster mode",
  "main": "app.js",
  "dependencies": {
    "address": "^1.1.2",
    "consul": "^0.30.0",
    "pm2": "^5.2.0",
    "pmx": "^1.6.7",
    "prom-client": "14.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/s00d/pm2-cluster-prometheus.git"
  },
  "config": {
    "app_name": "api,charge",
    "port": 3000,
    "reigster_disabled": false,
    "consul_service_name": "gleeman-api",
    "consul_host": "127.0.0.1",
    "consul_port": "8500",
    "register_mode": "cluster"
  },
  "apps": [
    {
      "merge_logs": true,
      "max_memory_restart": "500M",
      "script": "app.js"
    }
  ],
  "author": "Ruiyuan Wang",
  "license": "MIT"
}
